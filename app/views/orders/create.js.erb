const price = document.querySelector(".price").innerText;
const quantity = document.querySelector(".quantity").innerText;
const category = document.querySelector(".category").innerText;

$ ->
  $(".submit-order").click ->
    $.ajax({
      type: "POST",
      url: "/orders",
      data: { order: { quantity: quantity, category: category, total_price: price } },
      success:(data) ->
        alert data.id
        return false
      error:(data) ->
        return false
    })
